package day07;
import java.util.Random;
class HaHa{
	
}
class KiKi{
	
}
public class Sub02 {
	/*
	 *반환타입이 void가 아니라면
	 *1.반드시 매서드 내에 return이 존재해야 한다
	 *2.return문 뒤에는 반환타입이 받을 수 있는 값을 설정해야 한다. 
	 */
	int method01(){
		System.out.println();
		return 100;
	}
	char method02(){
		return 's';
	}	
	String method03(){
		return "성공";
	}
	Random method04(){
		Random r = new Random();
		return r;
	}
	
	int[] method05(){
		int[] arr = new int[2];
		return arr;
	}
	
	HaHa method06(new KiKi){
		return new HaHa();
	}
		
	

}
